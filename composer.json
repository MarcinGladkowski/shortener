{
    "require": {
        "php": ">=8.4",
        "symfony/framework-bundle": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/yaml": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "symfony/test-pack": "^1.1"
    }
}
